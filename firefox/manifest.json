{
  "manifest_version": 2,
  "name": "Hide dailies",
  "version": "1.0",

  "description": "I keep getting distracted by dailies during the day. Hide dailies from view.",

  "icons": {
    "48": "images/habitica-icon.png",
    "96": "images/habitica-icon.png"
  },

  "background": {
    "scripts": ["background.js"]
  },

  "browser_action": {
    "default_title": "Hide dailies"
  },

  "permissions": [
    "activeTab",
    "tabs"
  ]
}