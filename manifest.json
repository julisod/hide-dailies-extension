{
  "manifest_version": 3,
  "name": "Hide dailies",
  "description": "I keep getting distracted by dailies during the day. Hide dailies from view.",
  "version": "1.0",
  "icons": {
    "128": "images/habitica-icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": "images/habitica-icon.png"
  },
  "permissions": ["scripting", "activeTab"]
}
